{
  "name"            : "min-iterator",
  "version"         : "0.1.1",
  "description"     : "Minimal iterator API for Node.js and the browser",
  "keywords"        : ["iterator", "node", "browser"],
  "author"          : "Maximilian Antoni (http://maxantoni.de)",
  "homepage"        : "https://github.com/mantoni/min-iterator.js",
  "main"            : "./lib/iterator.js",
  "engines"         : {
    "node"          : ">=0.8"
  },
  "scripts"         : {
    "start"         : "mochify --watch --node --cover",
    "lint"          : "jslint --color -- ./**/*.js",
    "test-wd"       : "mochify --wd",
    "test"          : "npm run lint && mocha -R spec && mochify --cover"
  },
  "repository"      : {
    "type"          : "git",
    "url"           : "https://github.com/mantoni/min-iterator.js.git"
  },
  "devDependencies" : {
    "mocha"         : ">=1.17 <2",
    "jslint"        : "~0.2.7",
    "mochify"       : "~0.5.0"
  },
  "files"           : ["lib", "README.md", "CHANGES.md", "LICENSE"],
  "license"         : "MIT"
}
